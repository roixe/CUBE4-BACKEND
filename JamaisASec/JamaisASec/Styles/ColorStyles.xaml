<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--#region Main-->
    <SolidColorBrush x:Key="WindowBackground" Color="#f4eded"/>
    <SolidColorBrush x:Key="Burgundy" Color="#5c1111"/>
    <SolidColorBrush x:Key="LightBurgundy" Color="#711616"/>
    <SolidColorBrush x:Key="Taupe" Color="#ccabab"/>
    <SolidColorBrush x:Key="LightTaupe" Color="#d8c5c5"/>
    <SolidColorBrush x:Key="LightGray" Color="#dee4ec"/>
    <!--#endregion-->
    
    <!--#region etiquettes-->
    <SolidColorBrush x:Key="ForestGreen" Color="#00703C"/>
    <SolidColorBrush x:Key="OceanBlue" Color="#5fa9e1"/>
    <SolidColorBrush x:Key="CalmRed" Color="#cc0000"/>
    <SolidColorBrush x:Key="Mandarine" Color="#e69138"/>
    <!--#endregion-->
    
    <!--#region Forms-->
    <LinearGradientBrush x:Key="FormBackground" StartPoint="0,0" EndPoint="0,5">
        <GradientStop Color="#c45e5e" Offset="0.0"/>
        <GradientStop Color="#ffdbdb" Offset="0.5"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="FormBackgroundLayer" Color="#fff5eceb"/>
    <!--#endregion-->
</ResourceDictionary>